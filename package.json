{
   "name": "@sriler/realize",
   "workspaces": [
      "frontend",
      "sequence"
   ],
   "devDependencies": {
      "@types/jest": "^28.1.0",
      "@typescript-eslint/eslint-plugin": "^5.26.0",
      "@typescript-eslint/parser": "^5.26.0",
      "eslint": "^8.16.0",
      "eslint-config-prettier": "^8.5.0",
      "jest": "^28.1.0",
      "prettier": "2.6.2",
      "typescript": "^4.7.2"
   },
   "scripts": {
      "generate": "npm -w frontend run generate",
      "build": "npm -w sequence run build && npm -w frontend run build",
      "test": "npm -w sequence run test && npm -w frontend run test",
      "release": "npm run generate && npm run build && npm run test",
      "sequence:build": "npm -w sequence run build",
      "sequence:test": "npm -w sequence run test",
      "frontend:generate": "npm -w frontend run generate",
      "frontend:build": "npm -w frontend run build",
      "frontend:test": "npm -w frontend run test",
      "frontend:start": "npm -w frontend run start"
   }
}
